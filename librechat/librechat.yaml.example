version: 1.1.5

cache: true

endpoints:
  custom:
    - name: "CLIProxyAPI"
      # Use environment variable for API key (set API_KEY in Coolify)
      apiKey: "${API_KEY}"
      baseURL: "http://cli-proxy-api:8317/v1"
      models:
        default: []
        fetch: true
      titleConvo: true
      titleModel: "current_model"
      summarize: false
      forcePrompt: false
      modelDisplayLabel: "CLIProxyAPI"

registration:
  socialLogins: []

rateLimits:
  fileUploads:
    ipMax: 100
    ipWindowInMinutes: 60
    userMax: 50
    userWindowInMinutes: 60